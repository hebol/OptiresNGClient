{
  "expo": {
    "name": "OptiresNG Client",
    "description": "",
    "slug": "OptiresNGClient",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "0.3.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "optiresng",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0096FF"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 10000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "se.optiresng.client",
      "versionCode": 4
    },
    "ios": {
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch"
        ],
        "NSLocationAlwaysAndWhenInUseUsageDescription": "För bästa funktion behöver OptiresNG behöver tillgång till din position hela tiden",
        "NSLocationWhenInUseUsageDescription": "OptiresNG behöver tillgång till din position",
        "LSApplicationQueriesSchemes": [
          "apple-maps",
          "google-maps",
          "waze"
        ]
      },
      "bundleIdentifier": "se.optiresng.client",
      "buildNumber": "0.4.1",
      "config": {
        "usesNonExemptEncryption": false
      },
      "supportsTablet": false
    }
  }
}
